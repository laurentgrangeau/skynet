{
    "Version": "2012-10-17",
    "Statement": [
       {
          "Effect": "Allow",
          "Action": [
             "s3:ListBucket",
             "s3:GetBucketLocation",
             "s3:FullAccess"
          ],
          "Resource": [
             "arn:aws:s3:::skynet-linuxkit"
          ]
       },
       {
          "Effect": "Allow",
          "Action": [
             "s3:GetObject"
          ],
          "Resource": [
             "arn:aws:s3:::skynet-linuxkit/*"
          ]
       },
       {
          "Effect": "Allow",
          "Action":[
             "ec2:ModifySnapshotAttribute",
             "ec2:CopySnapshot",
             "ec2:RegisterImage",
             "ec2:Describe*",
             "ec2:FullAccess"
          ],
          "Resource": "*"
       }
    ]
 }